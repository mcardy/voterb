<p>
  <strong>Title: </strong>
  <%= @poll.title %>
</p>
<p>
  <% @poll.poll.each do |key, value| %>
    <strong><%= key %>: </strong><%= value[0] %>,
    <strong>votes: </strong><%= value[1] %><br>
  <% end %>
</p>

<%= link_to 'Vote', vote_poll_path %>
<%= link_to 'Edit', edit_poll_path %>
<%= link_to 'Back', polls_path %>
